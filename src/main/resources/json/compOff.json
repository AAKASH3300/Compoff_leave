{
  "createCompOff": {
    "body": [
      {
        "name": "employeeId",
        "required": true,
        "maxLength": 10,
        "format": "^[0-9]*$"
      },
      {
        "name": "requestType",
        "required": true,
        "maxLength": 20,
        "format": "^[a-zA-Z_]*$"
      },
      {
        "name": "requestedDate",
        "required": true,
        "maxLength": 8,
        "format": "^\\d{4}(0[1-9]|1[012])(0[1-9]|[12][0-9]|3[01])$"
      },
      {
        "name": "punchIn",
        "required": true,
        "maxLength": 5,
        "format": "([01]?[0-9]|2[0-3]):[0-5][0-9]"
      },
      {
        "name": "punchOut",
        "required": true,
        "maxLength": 5,
        "format": "([01]?[0-9]|2[0-3]):[0-5][0-9]"
      },
      {
        "name": "workHours",
        "required": true,
        "maxLength": 5,
        "format": null
      },
      {
        "name": "isRegularized",
        "required": true,
        "maxLength": 25,
        "format": null
      },
      {
        "name": "requestedFor",
        "required": true,
        "maxLength": 20,
        "format": null
      },
      {
        "name": "reason",
        "required": true,
        "maxLength": 500,
        "format": null
      }
    ]
  }
}